{
  "name": "ClayApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start --reset-cache",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "postinstall": "cd ios && pod install",
    "generate": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.13.3",
    "@react-native-community/art": "^1.2.0",
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-navigation/bottom-tabs": "^5.11.6",
    "@react-navigation/material-top-tabs": "^5.3.13",
    "@react-navigation/native": "^5.9.2",
    "@react-navigation/stack": "^5.14.1",
    "@reduxjs/toolkit": "^1.5.0",
    "@types/react-native-vector-icons": "^6.4.6",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "@urql/core": "^1.16.2",
    "@urql/devtools": "^2.0.2",
    "graphql": "^15.5.0",
    "react": "16.13.1",
    "react-native": "0.64.1",
    "react-native-appearance": "^0.3.4",
    "react-native-chatbot": "^0.0.1-alpha.12",
    "react-native-gesture-handler": "^1.9.0",
    "react-native-input-scroll-view": "^1.11.0",
    "react-native-loading-spinner-overlay": "^2.0.0",
    "react-native-neomorph-shadows": "^1.1.2",
    "react-native-read-more-text": "^1.1.2",
    "react-native-reanimated": "^1.13.2",
    "react-native-safe-area-context": "^3.1.9",
    "react-native-screens": "^2.15.2",
    "react-native-skeleton-content": "^1.0.20",
    "react-native-swipe-gestures": "^1.0.5",
    "react-native-tab-view": "^2.15.2",
    "react-native-unordered-list": "^1.0.4",
    "react-native-vector-icons": "^8.0.0",
    "react-native-webview": "^11.2.1",
    "react-redux": "^7.2.2",
    "styled-components": "^5.2.1",
    "styled-system": "^5.1.5",
    "urql": "^1.11.6"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@graphql-codegen/cli": "1.20.1",
    "@graphql-codegen/typescript": "^1.20.2",
    "@graphql-codegen/typescript-operations": "^1.17.14",
    "@graphql-codegen/typescript-urql": "^2.0.3",
    "@react-native-community/eslint-config": "^1.1.0",
    "@types/jest": "^25.2.3",
    "@types/react-native": "^0.63.2",
    "@types/react-native-loading-spinner-overlay": "^0.5.2",
    "@types/react-native-read-more-text": "^1.0.0",
    "@types/react-redux": "^7.1.16",
    "@types/react-test-renderer": "^16.9.2",
    "@types/styled-components": "^5.1.7",
    "@types/styled-components-react-native": "^5.1.0",
    "@types/styled-system": "^5.1.10",
    "babel-jest": "^25.1.0",
    "babel-plugin-graphql-tag": "^3.2.0",
    "babel-plugin-import-graphql": "^2.8.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^6.5.1",
    "jest": "^25.1.0",
    "metro-react-native-babel-preset": "^0.59.0",
    "react-test-renderer": "16.13.1",
    "typescript": "^3.8.3"
  },
  "rnpm": {
    "assets": [
      "./assets/fonts"
    ]
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "^assets/(.*)": "<rootDir>/assets/$1",
      "^hooks/(.*)": "<rootDir>/hooks/$1",
      "^components/(.*)": "<rootDir>/components/$1",
      "^constants/(.*)": "<rootDir>/constants/$1",
      "^containers/(.*)": "<rootDir>/containers/$1",
      "^navigation/(.*)": "<rootDir>/navigation/$1",
      "^screens/(.*)": "<rootDir>/screens/$1",
      "^selectors/(.*)": "<rootDir>/selectors/$1",
      "^state/(.*)": "<rootDir>/state/$1",
      "^gqlClient/(.*)": "<rootDir>/gqlClient/$1"
    }
  }
}
